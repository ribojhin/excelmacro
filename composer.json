{
    "name": "ribojhin/excelmacro",
    "description": "Modifier un fichier excel macro",
    "type": "library",
    "license": "MIT",
    "keywords": ["php", "starter kit"],
    "homepage": "https://github.com/ribojhin/excelmacro",
    "authors": [
      {
        "name": "ribojhin",
        "email": "ribojhin@gmail.com"
      }
    ],
    "autoload": {
      "psr-4": {
        "Ribojhin\\Excelmacro\\": ["src/"]
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Ribojhin\\Excelmacro\\": ["tests/"]
      }
    },
    "require": {
      "php": "^7.3"
    },
    "require-dev": {
      "phpunit/phpunit": "^8.4.3",
      "php-coveralls/php-coveralls": "^2.2.0",
      "squizlabs/php_codesniffer": "^3.5.2",
      "phpmd/phpmd" : "@stable",
      "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
      "slevomat/coding-standard": "^5.0"
    },
    "suggest": {
      "phpbench/phpbench": "PHPBench is a benchmarking framework for PHP",
      "infection/infection": "AST based PHP Mutation Testing Framework"
    },
    "scripts": {
      "test": "./vendor/bin/phpunit",
      "cover": "./vendor/bin/phpunit --coverage-clover ./build/phpunit/clover.xml",
      "coveralls": "./vendor/bin/php-coveralls -v",
      "check-style": "./vendor/bin/phpcs",
      "fix-style": "./vendor/bin/phpcbf",
      "bench": "phpbench run benchmarks/  --report=default",
      "infection": "infection"
    }
}